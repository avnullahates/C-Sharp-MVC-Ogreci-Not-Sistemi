@using OgrenciNotMvc.Models.EntityFramwork
@model List<TBLNOTLAR>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table class="table table-bordered">
    <tr>
        <th>NOT ID</th>
        <th>OGRENCI</th>
        <th>DERS</th>
        <th>SINAV 1</th>
        <th>SINAV 2</th>
        <th>SINAV 3</th>
        <th>PROJE</th>
        <th>ORTALAMA</th>
        <th>DURUM</th>
        <th>GUNCELLE</th>


    </tr>
    <tbody>
        @foreach (var snv in Model)
        {
        <tr>
            <td>@snv.NOTID</td>
            <td>@snv.OGRID</td>
            <td>@snv.DERSID</td>
            <td>@snv.SINAV1</td>
            <td>@snv.SINAV2</td>
            <td>@snv.SINAV3</td>
            <td>@snv.PROJE</td>
            <td>@snv.ORTALAMA</td>
            <td>@snv.DURUM</td>
            <td><a href="/Notlar/NotGetir/@snv.NOTID" class="btn btn-warning">Guncelle</a></td>


        </tr>
        }

    </tbody>

</table>


<a href="/Notlar/YeniSinav" class="btn btn-primary">Yeni Sinav Girisi</a>

