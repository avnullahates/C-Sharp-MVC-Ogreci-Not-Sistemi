@using OgrenciNotMvc.Models.EntityFramwork
@model List<TBLOGRENCILER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>Ogrenci ID</th>
        <th>ADI</th>
        <th>SOYADI</th>
        <th>CINSIYET</th>
        <th>KULUP</th>
        <th>ISLEMLER</th>
        
    </tr>
    <tbody>
        @foreach (var ogr in Model)
        {
            <tr>
                <td>@ogr.OGRENCIID</td>
                <td>@ogr.OGRAD</td>
                <td>@ogr.OGRSOYADI</td>
                <td>@ogr.OGRCINSIYET</td>
                <td>@ogr.OGRKULUP</td>
                <td><a href="/Ogrenci/Sil/@ogr.OGRENCIID" class="btn btn-danger">Sil</a>
                    <a href="/Ogrenci/OgrenciGetir/@ogr.OGRENCIID" class="btn btn-success">Guncelle</a>

                </td>
                
            </tr>
        }

    </tbody>

</table>

<a href="/Ogrenci/YeniOgrenci" class="btn btn-primary">Yeni Ogrenci Kaydi</a>
