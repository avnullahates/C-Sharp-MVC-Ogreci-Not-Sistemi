@using OgrenciNotMvc.Models.EntityFramwork;
@model List<TBLKULUPLER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>KULÜP ID</th>
        <th>KULÜP ADI</th>
        <th>ISLEMLER</th>
    </tr>
    <tbody>
        @foreach (var klp in Model)
        {
            <tr>
                <td>@klp.KULUPID</td>
                <td>@klp.KULUPAD</td>
                <td><a href="/Kulupler/Sil/@klp.KULUPID" class="btn btn-danger">Sil</a>
                <a href="/Kulupler/KulupGetir/@klp.KULUPID"  class="btn btn-success">Guncelle</a></td>
            </tr>
        }

    </tbody>

</table>.
<a href="/Kulupler/YeniKulup" class="btn btn-primary">Yeni Kulup Kaydi</a>

